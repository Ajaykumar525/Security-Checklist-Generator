    <div class="inner-conterner">
        <!-- inner-conterner -->
        <header class="site-header">
            <div class="header-inner">
                <!-- navigation panel -->
                <div class="container">
                    <div class="row">
                        <div class="header-table col-md-12">
                            <div class="brand">
                                @if (Session["UserName"] == null)
                                {
                                    <a href="/Home/Index">
                                        <img src="~/assest/images/logo.png" alt="Enfold">
                                    </a>
                                }
                                else
                                {
                                    <a href="/Dashboard/Dashboard">
                                        <img src="~/assest/images/logo.png" alt="Enfold">
                                    </a>
                                }
                            </div>
                            <nav id="nav-wrap" class="main-nav">
                                <div id="mobnav-btn"> </div>
                                <ul class="sf-menu">
                                    @if (Session["UserName"] == null)
                                    {
                                        <li>
                                            @Html.ActionLink("Home", "Index", "Home")
                                        </li>
                                        <li>
                                            @Html.ActionLink("Sign up", "Register", "Login")
                                        </li>
                                        <li>
                                            @Html.ActionLink("Sign in", "Login", "Login")
                                        </li>
                                    }
                                    else
                                    {
                                        <li class="title-un">
                                            <h5 style="font-size: 14px;letter-spacing: 2px;">@Session["UserName"].ToString()</h5>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Logout", "Login")">Logout</a>
                                        </li>
                                    }
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <!-- End navigation panel -->
                </div>
                <!-- End navigation panel -->
            </div>
        </header>
